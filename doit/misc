                //re: /^(?:\[((?:[@?$])?[\w\-]*)\s*(?:([\^$*~%!\/]?=)\s*(['"])?((?:\\\]|.)*?)\3)?(?!\\)\])/,

                matcher: /^(")?(?:[^\."])(?:(?:[\.])?(?:[\w\-!#$%&'*+\/=?\^_`{|}~]))*\1@(\w[\-\w]*\.){1,5}([A-Za-z]){2,6}$/

                    topRe: /(?:(\\{\\%)|(\{\\[)|\\{([^{}]+)\\})|(?:<tpl([^>]*)\\>)|(?:<\\/tpl>)/g,
    actionsRe: /\\s*(elif|elseif|if|for|foreach|exec|switch|case|eval|between)\\s*\\=\\s*(?:(?:"([^"]*)")|(?:'([^']*)'))\\s*/g,
    propRe: /prop=(?:(?:"([^"]*)")|(?:'([^']*)'))/,
    defaultRe: /^\\s*default\\s*$/,
    elseRe: /^\\s*else\\s*$/